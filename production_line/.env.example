# AWS Region where your resources are located
AWS_REGION=us-west-2

# AWS Profile to use (from ~/.aws/credentials)
AWS_PROFILE=your-profile-name

# AWS Account ID (12-digit number)
AWS_ACCOUNT_ID=123456789012

# DynamoDB table name for storing device information
DYNAMODB_TABLE_NAME=dobby-devices

# AWS Sidewalk Configuration
# Get these values from your AWS IoT Wireless console
SIDEWALK_DEVICE_PROFILE_ID=your-sidewalk-profile-id
SIDEWALK_DESTINATION_NAME=your-sidewalk-destination

# Optional: AWS Credentials (if not using AWS CLI configuration)
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key

NCS_PATH=/opt/nordic/ncs/v2.5.0
FIRMWARE_HEX_PATH=
